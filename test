


t:=10000:

interface(rtablesize = 100):
r:=6:
lambda:= 0.5:

A := AGenerator(r, t):
At := Transpose(A):


Y0 := Vector(3*r+1, symbol = y):
Y1:= Tropical_y(lambda,r):
Y:= <Y1,0,0>:
beta := `<,>`(2^6, 2^6, 2^5, 2^5, 2^5, 2^4, 2^4, 2^4, 2^3, 2^3, 2^3, 2^2, 2^2, 2^2, 2, 2, 2, 1, 1):

f := proc (x) options operator, arrow; t^x end proc:
v:= convert(map(f, Y),Vector):
V:= `~`[`*`](beta, v):

evalf(Multiply(At, V));
